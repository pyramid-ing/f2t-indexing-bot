  },
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\" --fix && yarn --cwd ./backend lint",
    "start": "concurrently \"yarn start:renderer\" \"wait-on tcp:8080 && yarn start:main\"",
    "start:main": "cross-env NODE_OPTIONS=--experimental-modules tsc -p tsconfig.electron.json && electron .",
    "start:renderer": "cross-env NODE_ENV=development webpack serve --config webpack.config.js",
    "debug": "rimraf dist/* && concurrently \"yarn debug:renderer\" \"wait-on tcp:8080 && yarn debug:main\"",
    "debug:renderer": "cross-env ELECTRON_DEBUG=true NODE_ENV=development webpack serve --config webpack.config.js",
    "debug:main": "cross-env tsc -p tsconfig.electron.json && cross-env ELECTRON_DEBUG=true electron . --remote-debugging-port=9222",
    "clean": "rimraf dist/*",
    "build": "yarn clean && yarn build:main && yarn build:renderer && yarn --cwd backend build",
    "build:main": "tsc -p tsconfig.electron.prod.json",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode production",
    "package": "yarn build && electron-builder --config electron-builder.json",
    "package-mac": "yarn build && electron-builder --config electron-builder.json --mac",
    "package-win": "yarn build && electron-builder --config electron-builder.json --win --publish=always"
  },
  "dependencies": {
